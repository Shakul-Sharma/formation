pipeline {
    agent any
    parameters {
  choice choices: ['qa' , 'dev' , 'stg' , 'prod'], name: 'environment '
  string description: 'master , dev', name: 'BRANCH'
}

    stages {
        stage('Create Stack') {
            steps {
            sh "aws cloudformation create-stack --stack-name s3bucket2 --template-body file://s3cft.json --region 'us-east-1'"
              }
             }
            }
            }
